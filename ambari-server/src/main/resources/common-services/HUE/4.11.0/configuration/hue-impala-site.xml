<configuration supports_final="true">
  <property>
	<name>enable_impala</name>
	<description>Enable the impala service</description>
	<value>true</value>

  </property>

  <property>
	<name>impala_host</name>
	<description>The host of the Impala Server (Impalad)</description>
	<value>localhost</value>

  </property>
  <property>
	<name>impala_port</name>
	<description>Port of the impala server</description>
	<value>21050</value>
    <value-attributes>
	  <type>int</type>
	  <empty-value-valid>true</empty-value-valid>
    </value-attributes>
  </property>  
</configuration>
